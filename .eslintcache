ut.timestamp === \"\" || workout.workoutMood === \"\" || workout.logEntry === \"\")  {\n          window.alert(\"Please fill out the name and address fields\")\n      } else {\n  \n        setIsLoading(true);\n        if (workoutId){\n          updateWorkout({\n            id: parseInt(workout.id),\n            userId: parseInt(user),\n            timestamp: Date.now(),\n            name: workout.name,\n            workoutMood: workout.workoutMood,\n            logEntry: workout.logEntry,\n          })\n          .then(() => history.push(`/workouts/detail/${workout.id}`))\n        }else {\n         \n          addWorkout({\n            id: parseInt(workout.id),\n            userId: parseInt(user),\n            timestamp: Date.now(),\n            name: workout.name,\n            workoutMood: workout.workoutMood,\n            logEntry: workout.logEntry,\n          })\n          .then(() => history.push(\"/workouts\"))\n        }\n      }\n    }\n\n    useEffect(() => {\n\n        if (workoutId){\n          getWorkoutById(workoutId)\n          .then(workout => {\n              setWorkout(workout);\n              setIsLoading(false);\n          })\n        } else {\n          setIsLoading(false);\n        }\n    }, \n    []);\n\n    //since state controlls this component, we no longer need\n    //useRef(null) or ref\n\n    return (\n      <form className=\"workoutForm\">\n        <h2 className=\"workoutForm__title\">{workoutId ? <>Edit Workout</> : <>New Workout</>}</h2>\n        <fieldset>\n          <div className=\"form-group\">\n            <label htmlFor=\"name\">Workout name: </label>\n            <input type=\"text\" id=\"name\" name=\"name\" required autoFocus className=\"form-control\"\n            placeholder=\"workout name\"\n            onChange={handleControlledInputChange}\n            defaultValue={workout.name}/>\n          </div>\n        </fieldset>\n        <fieldset>\n          <div className=\"form-group\">\n            <label htmlFor=\"date\">Workout date: </label>\n            <input type=\"date\" id=\"workoutDate\" name=\"workoutDate\" required autoFocus className=\"form-control\"\n            placeholder=\"Workout Date\"\n            onChange={handleControlledInputChange}\n            value={workout.Date}/>\n          </div>\n        </fieldset>\n        <fieldset>\n          <div className=\"form-group\">\n              <label htmlFor=\"focus\">Journal:</label>\n              <textarea type=\"text\" name=\"logEntry\" id=\"logEntry\" onChange={handleControlledInputChange} required autoFocus className=\"form-control\" placeholder=\"Entry\" value={workout.logEntry}/>\n          </div>\n        </fieldset>\n        <fieldset>\n        <div className=\"form-group\">\n              <label htmlFor=\"workoutMood<\">Workout Mood:</label>\n              <input type=\"text\" name=\"workoutMood\" id=\"workoutMood\" onChange={handleControlledInputChange} required autoFocus className=\"form-control\" placeholder=\"Mood\" value={workout.workoutMood}/>\n        </div>\n        </fieldset>\n        <button className=\"btn btn-primary\"\n          disabled={isLoading}\n          onClick={event => {\n            event.preventDefault(); // Prevent browser from submitting the form and refreshing the page\n            handleSaveWorkout();\n          }}>\n     {workoutId ? <>Save Workout</> : <>Add Workout</>}</button> \n          \n      </form>\n    )\n} ","/Users/BenKimball/workspace/fitnessforward/src/components/exercise/ExerciseForm.js",["80","81","82"],"/Users/BenKimball/workspace/fitnessforward/src/components/workout/WorkoutDetail.js",["83"],"/Users/BenKimball/workspace/fitnessforward/src/components/exercise/ExerciseDetail.js",["84","85","86","87","88","89","90","91"],"/Users/BenKimball/workspace/fitnessforward/src/components/exercise/ExerciseProvider.js",[],"/Users/BenKimball/workspace/fitnessforward/src/components/workout/WorkoutProvider.js",[],"/Users/BenKimball/workspace/fitnessforward/src/components/exercise/Exercis